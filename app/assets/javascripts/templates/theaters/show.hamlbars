.page-header
  %h1
    = hb 'name'
%h2
  %small= 'Films'
%table.table.table-striped
  %tr
    %th= 'ID'
    %th= 'Film'
    %th= 'Showtimes'
  = hb 'each showing in showings' do
    %tr
      %td
        = hb 'showing.film.id'
      %td
        = hb "linkTo 'films.show' showing.film" do
          = hb 'showing.film.title'
      %td
        = hb 'showing.showtime'
= hb "linkTo 'theaters'", class: 'button' do
  Back
